{
  "name": "wallet-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "typeorm": "npx typeorm-ts-node-commonjs",
    "migration:create": "npx typeorm migration:create src/migration",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate src/migration/updateTable -d src/data-source.ts",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d src/data-source.ts",
    "db:create": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:create",
    "db:drop": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:drop",
    "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.10",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.3.10",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.3.10",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/typeorm": "9.0.1",
    "@sendgrid/mail": "^7.7.0",
    "@solana-suite/core": "0.5.9",
    "@solana-suite/nft": "0.5.9",
    "@solana-suite/shared": "0.5.8",
    "@solana-suite/storage": "0.5.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.5",
    "crypto-js": "^4.1.1",
    "csurf": "^1.11.0",
    "helmet": "^6.0.1",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.2.0",
    "node-rsa": "^1.1.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.0",
    "rxjs": "^7.8.0",
    "stripe": "^11.15.0",
    "typeorm": "^0.3.9",
    "typeorm-extension": "^2.5.4",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.3.10",
    "@types/bcrypt": "^5.0.0",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.3",
    "@types/passport-jwt": "^3.0.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.55.0",
    "bcrypt": "^5.0.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
