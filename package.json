{
  "name": "wallet-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "typeorm": "npx typeorm-ts-node-commonjs",
    "migration:create": "npx typeorm migration:create src/migration",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate src/migration/updateTable -d src/data-source.ts",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d src/data-source.ts",
    "db:create": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:create",
    "db:drop": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:drop",
    "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.12",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^9.3.12",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/throttler": "^5.1.1",
    "@nestjs/typeorm": "10.0.1",
    "@sendgrid/mail": "^8.1.0",
    "@solana-suite/airdrop": "0.7.11",
    "@solana-suite/compressed-nft": "0.7.11",
    "@solana-suite/config": "0.7.11",
    "@solana-suite/memo": "0.7.11",
    "@solana-suite/multisig": "0.7.11",
    "@solana-suite/regular-nft": "0.7.11",
    "@solana-suite/sol-native": "0.7.11",
    "@solana-suite/spl-token": "0.7.11",
    "@solana-suite/storage": "^0.7.11",
    "@solana-suite/utils": "0.7.11",
    "aws-sdk": "^2.1572.0",
